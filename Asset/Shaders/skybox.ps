TextureCube tSkyboyMap;
SamplerState tSkyboxSamplerState;

struct v2p {
	float4 position : SV_POSITION;
	float3 texcood	: TEXCOOD;
};


float4 main(v2p input) : SV_TARGET
{
	float4 color = tSkyboyMap.Sample(tSkyboxSamplerState, input.texcood);
	color = pow(color, 1/2.2);
    return float4(color.rgb, 1.0);
}

